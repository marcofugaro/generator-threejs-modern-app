{
  "name": "<%= title %>",
  "version": "0.0.0",
  "description": "<%= description %>",<% if (githubUsername) { %>
  "repository": "https://github.com/<%= githubUsername %>/<%= title %>",<% } %>
  "author": {
    "name": "<%= name %>",
    "email": "<%= email %>"<% if (website) { %>,
    "url": "<%= website %>"<% } %>
  },
  "license": "MIT",
  "scripts": {
    "postinstall": "yarn install-tree",
    "install-tree": "hash brew && ! hash tree && brew install tree || true",
    "start": "NODE_ENV=development webpack-dev-server",
    "prebuild": "yarn clean; yarn copy-public",
    "build": "NODE_ENV=production webpack",
    "clean": "rm -rf build",
    "copy-public": "cp -r public build",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build"
  },
  "browserslist": {
    "production": [
      ">0.25%",
      "not ie 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 Chrome version",
      "last 1 Firefox version",
      "last 1 Safari version"
    ]
  },
  "dependencies": {
    "cannon": "^0.6.2",
    "canvas-sketch-util": "^1.10.0",
    "controls-gui": "^2.0.0",
    "controls-state": "^2.0.0",
    "datauritoblob": "^1.0.0",
    "detect-gpu": "^1.2.0",
    "glsl-vignette": "^1.1.0",
    "image-promise": "^7.0.0",
    "lodash": "^4.17.15",
    "orbit-controls": "^1.2.4",
    "p-map": "^3.0.0",
    "pretty-ms": "^6.0.0",
    "stats.js": "^0.17.0",
    "three": "0.113.2",
    "touches": "^1.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "babel-loader": "^8.0.6",
    "babel-preset-accurapp": "^5.0.0",
    "chalk": "^3.0.0",
    "detect-port": "^1.3.0",
    "eslint-config-accurapp": "^5.0.0",
    "event-hooks-webpack-plugin": "^2.1.5",
    "gh-pages": "^2.2.0",
    "glslify-loader": "^2.0.0",
    "html-webpack-plugin": "4.0.0-beta.11",
    "indent-string": "^4.0.0",
    "raw-loader": "^4.0.0",
    "react-dev-utils": "^10.2.0",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "3.10.3",
    "webpack-merge": "^4.2.2"
  }
}

{
  "name": "<%= title %>",
  "version": "0.0.0",
  "description": "<%= description %>",<% if (githubUsername) { %>
  "repository": "https://github.com/<%= githubUsername %>/<%= title %>",<% } %>
  "author": {
    "name": "<%= name %>",
    "email": "<%= email %>"<% if (website) { %>,
    "url": "<%= website %>"<% } %>
  },
  "license": "MIT",
  "scripts": {
    "postinstall": "yarn install-tree",
    "install-tree": "hash brew && ! hash tree && brew install tree || true",
    "start": "NODE_ENV=development webpack-dev-server",
    "prebuild": "yarn clean; yarn copy-public",
    "build": "NODE_ENV=production webpack",
    "clean": "rm -rf build",
    "copy-public": "cp -r public build",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build"
  },
  "browserslist": {
    "production": [
      ">0.25%",
      "not ie 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 Chrome version",
      "last 1 Firefox version",
      "last 1 Safari version"
    ]
  },
  "dependencies": {
    "canvas-sketch-util": "^1.8.0",
    "controls-gui": "^1.2.2",
    "controls-state": "^1.1.1",
    "datauritoblob": "^1.0.0",
    "glsl-vignette": "^1.1.0",
    "image-promise": "^6.0.2",
    "lodash": "^4.17.11",
    "orbit-controls": "^1.2.4",
    "p-map": "^2.1.0",
    "pretty-ms": "^4.0.0",
    "stats.js": "^0.17.0",
    "three": "^0.103.0",
    "touches": "^1.2.2"
  },
  "devDependencies": {
    "babel-loader": "^8.0.5",
    "babel-preset-accurapp": "^4.1.2",
    "chalk": "^2.4.2",
    "eslint-config-accurapp": "^4.2.4",
    "event-hooks-webpack-plugin": "^2.1.1",
    "fs-extra": "^7.0.1",
    "gh-pages": "^2.0.1",
    "glob": "^7.1.3",
    "glslify-loader": "^2.0.0",
    "html-webpack-plugin": "4.0.0-beta.5",
    "indent-string": "^4.0.0",
    "outdent": "^0.7.0",
    "raw-loader": "^2.0.0",
    "react-dev-utils": "^9.0.0",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.3.1",
    "webpack-merge": "^4.2.1"
  }
}
